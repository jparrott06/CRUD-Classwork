<div class="form-data">
    <form action="/users/create" method="POST">
    
    <label for="txtFirstName">First Name</label>
    <input type="text" name="first" id="txtFirstName" autofocus required>

    <label for="txtLastName">Last Name</label>
    <input type="text" name="last" id="txtLastName" autofocus required>

    <label for="txtEmail">Email Address</label>
    <input type="email" name="email" id="txtEmail" required>

    <label for="txtPassword">Password</label>
    <input type="password" name="password" id="txtPassword" required>

    <label for="txtZipCode">Zip Code</label>
    <input type="text" name="zipCode" id="txtZipCode" required>

    <button class="button" type="submit">Create</button>
    
    </form>
</div>